---
layout : null
sitemap: false
---

[
{% for post in site.posts %}
    {
      "title"    : "{{ post.title }}",
      "url"     : "{{ site.url }}{{ post.url }}",
      "image"     : "{{ site.url }}/images/{{ post.image }}",
      "date"     : "{{ post.date | date: "%B %d, %Y" }}"
    } {% if forloop.last %}{% else %},{% endif %}
{% endfor %}
]