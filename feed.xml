---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom" xmlns:content="http://purl.org/rss/1.0/modules/content/" xmlns:webfeeds="http://webfeeds.org/rss/1.0">

  <channel>
    <title>{{ site.title | xml_escape }}</title>
    <description>{{ site.description | xml_escape }}</description>
    <link>{{ site.url }}{{ site.baseurl }}</link>
    <atom:link href="{{ "/feed.xml" | prepend: site.url }}" rel="self" type="application/rss+xml" />
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    <generator>Jekyll v{{ jekyll.version }}</generator>    
    <webfeeds:cover image="http://ahmetcadirci.com.tr/images/ahmetcadirci-wallpaper.jpg" />
    <webfeeds:icon>http://ahmetcadirci.com.tr/images/favicons/safari-pinned-tab.svg</webfeeds:icon>
    <webfeeds:logo>http://ahmetcadirci.com.tr/images/favicons/safari-pinned-tab.svg</webfeeds:logo>
    <webfeeds:accentColor>252525</webfeeds:accentColor>
    <webfeeds:related layout="card" target="browser"/>

    {% for post in site.posts limit:50 %}
      <item>
        <title>{{ post.title | xml_escape }}</title>
        <description>{{ post.content | xml_escape }}
        <hr>
        <table>
        <tr>
        <td>
        <img src="https://ahmetcadirci.com.tr/images/favicons/apple-touch-icon-76x76.png">
        </td>
        <td>
        <p style="margin: 0 0 5px 0;"><strong>Ahmet Çadırcı</strong></p>
        <p style="margin: 0 0 5px 0;"><a href="http://ahmetcadirci.com.tr">Diğer yazılar için tıklayın.</a></p>
        <p style="margin: 0 0 5px 0;"><a href="https://twitter.com/ahmetcadirci25">Twitter</a> | <a href="https://www.youtube.com/ahmetcadirci25">YouTube</a> | <a href="https://www.instagram.com/ahmetcadirci25/">Instagram</a> | <a href="http://feeds.feedburner.com/ahmetcadirci25">Feed</a></p>
        </td>
        </tr>
        </table>
        </description>
        <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
        <link>{{ post.url | prepend: site.url }}?utm_source=feedburner&amp;utm_medium=feed&amp;utm_campaign={{ post.title }}</link>
        <guid isPermaLink="true">{{ post.url | prepend: site.url }}?utm_source=feedburner&amp;utm_medium=feed&amp;utm_campaign={{ post.title }}</guid>
        {% for tag in post.tags %}
        <category>{{ tag | xml_escape }}</category>
        {% endfor %}
        {% for cat in post.categories %}
        <category>{{ cat | xml_escape }}</category>
        {% endfor %}
      </item>
    {% endfor %}
  </channel>
</rss>
